<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet author="James" id="201906301440">
        <createTable tableName="student">
            <column autoIncrement="true" name="id" type="BIGINT">
                <constraints primaryKey="true"/>
            </column>
            <column name="name" type="VARCHAR(255)"/>
            <column name="last_name" type="VARCHAR(255)" />
            <column name="phone" type="VARCHAR(255)"/>
            <column name="school_id" type="INT"/>
            <column name="creation_date" type="datetime"/>
        </createTable>
    </changeSet>

    <changeSet author="James" id="201907011621">
        <insert tableName="student">
            <column name="name" value="Ipsum"/>
            <column name="last_name" value="Dolor"/>
            <column name="phone" value="778899000"/>
            <column name="school_id" value="1"/>
        </insert>
    </changeSet>
</databaseChangeLog>
